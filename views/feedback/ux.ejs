      <span class="navbar-toggler-icon"></span>
    </button> -->
            <!-- <div class="collapse navbar-collapse" id="navbarNavDarkDropdown">
                            <ul class="navbar-nav">
                                <li class="nav-item dropdown"> -->
            <a class="nav-link" href="/feedback/ux?sort=mostup" role="button" data-bs-toggle="dropdown" aria-expanded="false">
              Sort by :<strong id="menu-item">Most Upvotes</strong>
              <span><img class="down-arrow" src="/assets/shared/icon-arrow-down.svg" alt="" srcset=""></span>

            </a>
            <ul id="mydropdown" class="dropdown-menu" aria-labelledby="dropdownMenuButton1">
              <li>
                <a class="dropdown-item" id="mostup" href="/feedback/ux?sort=mostup">Most Upvotes
                  <img class='checked' id="mostCheckUp" onclick="checkMark();" src="/assets/shared/icon-check.svg" alt="" src="">
                </a>
              </li>
              <hr class="dropdown-divider ">
              <li>
                <a class="dropdown-item" id="leastup" href="/feedback/ux?sort=leastup">Least Upvotes
                  <img class='checked' id="leastCheckUp" onclick="checkMark();" src="/assets/shared/icon-check.svg" alt="" src="">
                </a>
              </li>
              <hr class="dropdown-divider">
              <li><a class="dropdown-item" id="mostcomm" href="/feedback/ux?sort=mostcomm ">Most Comments<img class='checked' id="mostCheckComm" onclick="checkMark();" src="/assets/shared/icon-check.svg" alt="" src=""></a></li>
              <hr class="dropdown-divider">
              <li><a class="dropdown-item" id="leastcomm" href="/feedback/ux?sort=leastcomm ">Least Comments<img class='checked' id="leastCheckComm" onclick="checkMark();" src="/assets/shared/icon-check.svg" alt="" src=""></a></li>
            </ul>
            <!-- </li>
                            </ul>
                        </div> -->
          </div>
                        <a class="add-feedback-top text-center" href="/feedback/new " type="button ">+Add Feedback</a>


      </div>
      </nav>
    </div>
  </div>
  <!-- </div> -->

  <div class="feedback-screen-active">
<div class="overlay"></div>

    <!-- for(let [key, value] of Object.entries(request)) -->
    <% for(let req of Object.values(request)){ %>
    <div class="feedback-suggestion">
      <div class="d-flex flex-row feedback-suggested">
        <form id="vote" onsubmit="event.preventDefault();">
          <button id="content" class="btn feedback-suggested-numvotes" onclick="upvoteSuggestion('<%= req._id %>')">
            <img class="dropupCaret" src="/assets/shared/icon-arrow-up.svg" alt="" srcset="">
            <div id="votes-<%= req._id %>">
              <%= req.upvotes %>
            </div>
          </button>
        </form>



        <a class="feedback-navlink " href="/feedback/<%=req._id%>">
          <div class="feeback-title-para">
            <div id="counter" class="feedback-suggested-title">
              <%=req.title%>

            </div>
            <div class="feedback-suggested-para">
              <%=req.description%>
            </div>
            <div class="comment-icon">
              <img src="/assets/shared/icon-comments.svg" alt="" srcset="">

              <span class="comment-num"><%=req.comments.length %></span>
            </div>
          </div>
          <button class="btn-suggestion"><%=req.category%></button>
        </a>
      </div>
    </div>
    <% } %>
  </div>
  </div>

  </div>
  </div>



  <!-- Sidebar end -->
  <script src="/js/script.js"></script>
  <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>

